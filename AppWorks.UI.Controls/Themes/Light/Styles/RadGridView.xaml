                        <!--<Rectangle x:Name="NavigatorIndicatorBackground"
                                   Width="2"
                                   HorizontalAlignment="Right"
                                   Fill="{StaticResource BuyerColor}"
                                   ToolTip="Online"
                                   Visibility="{Binding RelativeSource={RelativeSource FindAncestor,
