                        <Rectangle x:Name="NavigatorIndicatorBackground2"
                                   Width="2"
                                   HorizontalAlignment="Right"
                                   Fill="DarkGray"
                                   ToolTip="Offline"
                                   Visibility="{Binding RelativeSource={RelativeSource FindAncestor,
                                                                                       AncestorType=telerik:GridViewRow},
