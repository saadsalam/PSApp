<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:tr="http://schemas.telerik.com/wpf" x:Class="AppWorks.UI.View.Reports.VehicleSummary" 
             xmlns:telerikReporting="clr-namespace:Telerik.Reporting;assembly=Telerik.Reporting"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <Grid>

        <tr:ReportViewer  Margin="10" x:Name="ReportViewer1" HorizontalAlignment="Stretch">
            <tr:ReportViewer.ReportSource>
                <telerikReporting:TypeReportSource TypeName="Appworks.Reports.Report1, AppWorks.Reports"/>
            </tr:ReportViewer.ReportSource>
        </tr:ReportViewer>


    </Grid>
</UserControl>
